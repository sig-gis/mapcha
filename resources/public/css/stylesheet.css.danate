/* CSS alpha version 0.2 Danate and GJohnson 2016-08-11 */

@import url(http://fonts.googleapis.com/css?family=Open+Sans:100,400,700|Oswald:normal|Cabin+Sketch:normal|Lobster:normal);

/* Animation */

@keyframes iMapcha {
    from {color:red;}
    to {color:yellow;}
}

/********************/
/* Universal Styles */
/********************/

body {
    font-family:Open Sans;
    font-size:16px;
    height:100vh;
    background-image:url(../img/chaBG.jpg);
    -webkit-background-size:cover;
    -moz-background-size:cover;
    -o-background-size:cover;
    background-size:cover;
}

header, nav, section, footer {
    display:block;
}

h1, h2, h3, h4, h5, h6, h7, p, label {
    font-family:Open Sans;
    font-weight:400;
}

.button {
    line-height:100% !important;
    text-overflow:ellipsis;
    cursor:pointer;
    background-color:#3FABC6 !important;
    color:white;
    font-family:Oswald !important;
    text-transform:uppercase;
    /* Safari 3-4, iOS 1-3.2, Android 1.6- */
    -webkit-border-radius:0px !important;
    /* Firefox 1-3.6 */
    -moz-border-radius:0px !important;
    /* Opera 10.5, IE 9, Safari 5, Chrome, Firefox 4, iOS 4, Android 2.1+ */
    border-radius:0px !important;
}

header {
    position:fixed;
    top:0rem;
    left:0rem;
    height:5.5rem;
    width:100%;
    color:#eee;
    background-color:white;
    background-image:url(http://www.adpc.net/igo/images/SERVIR/usaid.png),
                     url(http://www.adpc.net/igo/images/SERVIR/nasa.png),
                     url(http://www.adpc.net/igo/images/SERVIR/ADPC.jpg),
                     url(http://www.adpc.net/igo/images/SERVIR/servir.png);
    background-repeat:no-repeat;
    background-attachment:fixed;
    background-position:1rem 15px, 9rem 15px, 12rem 15px, calc(100% - 1rem) 15px;
    background-size:auto 35px, auto 35px, auto 35px, auto 25px;
    z-index:100;
}

header a:link,
header a:visited,
header a:active {
    color:black;
    text-decoration:none;
}

footer a:link,
footer a:visited,
footer a:active {
    color:#eee;
    text-decoration:none;
}

header a:hover,
footer a:hover,
header a.active-link {
    color:#555;
}

nav {
    position:absolute;
    top:3.6rem;
    left:1rem;
}

nav ul li {
    display:inline;
    list-style-type:none;
    padding:0.4rem;
}

#login-info {
    position:absolute;
    top:3.2rem;
    right:1rem;
}

#login-info p {
    color:black !important;
    font-size:0.8rem;
}

#login-info p a {
    padding:0.4rem;
    border:1px solid #999;
    color:black;
}

#content {
    position:fixed;
    top:5.5rem;
    left:0rem;
    height:calc(100vh - 5.5rem - 2.5rem);
    width:100%;
    background-color:rgba(255,255,255,0.2);
}

footer {
    position:fixed;
    bottom:0rem;
    left:0rem;
    height:2.5rem;
    width:100%;
    background-color:#1a1a1a;
}

footer p {
    padding:0.7rem 2.4rem;
    font-size:0.9rem;
}

/********/
/* Home */
/********/

#splash-image {
    display:none;
}

#left-block {
    position:absolute;
    right:5rem !important;
    height:100%;
    width:350px;
    border-radius:0rem !important;
    padding:2rem;
    background:rgba(255, 255, 255, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
}

#left-block h1 {
    margin:20px 0px;
    padding:0.5rem 0.5rem 0.5rem 1rem;
    font-size:3rem;
    font-weight:bold;
    color:gray;
    text-transform:uppercase;
    line-height:80%;
}

#left-block h1 em {
    font-size:0.8em !important;
    font-weight:400;
}

#left-block h2 {
    padding:0.5rem 0.5rem 0.75rem 1rem;
    color:#3C948B;
    font-family:Open Sans;
    font-weight:400;
    font-size:1.1rem;
    font-style:normal;
}

#right-block {
    display:none;
}

#home-content p {
    display:none;
}

.trademark {
    font-style:italic;
}

/*********/
/* About */
/*********/

#about {
    position:absolute;
    right:5rem;
    height:100%;
    width:350px;
    padding:2rem;
    background:rgba(255, 255, 255, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
}

/*********/
/* Login */
/*********/

#login-form {
    position:absolute;
    right:5rem;
    height:100%;
    width:350px;
    padding:2rem;
    background:rgba(255, 255, 255, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
}

#login-form h1 {
    font-size:1.9rem;
    font-weight:100;
    padding:0.5rem;
    color:gray !important;
}

#login-form input:not(.button) {
    height:2rem;
    width:20rem;
    border:1px solid #ececec;
    margin:0.3rem;
    padding:0px 5px;
}

#login-form input:not(.button):focus {
    border:1px solid DarkGrey;
    cursor:grab;
}

#login-form input:not(.button):hover {
    cursor:pointer;
    border:1px solid gray;
}

#login-form p#forgot-password a,
#login-form p#register-account a {
    display:block;
    font-size:0.9rem;
    font-weight:700;
    margin:0.5rem 0.5rem 0rem 1rem;
    color:gray;
    text-decoration:none;
}

#login-form input.button {
    height:2rem;
    width:21rem;
    margin:0.6rem 0.3rem;
    padding:0.5rem;
    border:1px solid #ececec;
}

#login-form input.button:focus {
    border:1px solid #999;
}

#login-form hr {
    height:12px;
    border:0;
    box-shadow:inset 0 12px 12px -12px rgba(0, 0, 0, 0.5);
}

/************/
/* Register */
/************/

