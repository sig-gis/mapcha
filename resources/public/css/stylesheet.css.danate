/***************/
/* Error Pages */
/***************/

p.alert {
    float:left;
    clear:left;
    margin:1rem 0rem 0rem 1rem;
    font-size:1rem;
    font-style:italic;
    width:35rem;
    padding:1rem;
    border:1px solid #009;
    border-radius:1rem;
    background:rgba(255, 255, 255, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
}

p.error-message {
    margin-top:1rem;
    font-size:1rem;
    font-style:normal;
    color:#555;
}

#access-denied, #page-not-found, #error-page {
    position:absolute;
    right:5rem;
    height:100%;
    width:350px;
    padding:2rem;
    background:rgba(255, 255, 255, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#CCFFFFFF, endColorstr=#CCFFFFFF);
}

#access-denied, #page-not-found, #error-page h3 {
    font-size:1.5rem;
    font-weight:100;
    color:gray;
}

/*************/
/* Dashboard */
/*************/

#dashboard {
    height:100%;
    padding:0rem;
    background:rgba(255,255,255, 0.8);
}

#dashboard h1 {
    display:none;
    font-size:1.5rem;
    font-weight:100;
    padding-left:1rem !important;
    width:calc(70% - 6.4rem);
    width:calc(70%);
    color:orange!important;
    line-height:1.2;
}

#dashboard #quit-button {
    display:inline-block;
    margin-top:0;
    width:5rem !important;
    position:fixed;
    left:calc(70vw - 1 * 5rem);
    z-index:10000;
    border-radius:0rem !important;
    background-color:#3FABC6 !important;
    height:40px;
    border:none;
    font-size:17;
    text-align:center;
}

#dashboard #image-analysis-pane{
    float:left;
    height:100%;
    width:70%;
    background-color:darkgray;
}

#dashboard .ol-dragbox {
    background-color:rgba(255,255,255,0.4);
    border-color:rgba(100,150,0,1);
}

#dashboard .ol-rotate {
    top:4rem;
}

#dashboard #sidebar {
    float:right;
    height:100%;
    width:30%;
    padding:0rem;
    overflow:scroll;
}

#dashboard #sidebar-contents {
    height:100%;
}

#dashboard #sidebar-contents fieldset {
    margin-top:0px;
}

#dashboard #sidebar-contents legend {
    font-size:1.2em;
    font-family:Oswald;
    font-weight:400 !important;
    padding:10px 0;
    color:#3FABC6;
    text-transform:uppercase;
    margin-top:-5px;
    text-indent:5px;
}

#dashboard #sidebar-contents select {
    font-family:Verdana,Geneva,sans-serif;
    font-weight:200;
    font-size:0.9em;
    line-height:90%;
    padding:0rem;
    height:1.8rem;
    width:100%;
    border:none;
    outline:none;
    color:#3D3B39;
    text-indent:5px;
}

#dashboard #sidebar-contents option {
    color:#3D3B39;
    text-indent:5px;
}

#dashboard #sidebar-contents input[name=new-plot] {
    height:40px;
    width:100%;
    margin:4px 0;
    font-size:1em !important;
}

#dashboard #sidebar-contents input[name=save-values] {
    clear:both;
    height:40px;
    width:100%;
    margin:4px 0;
    font-size:1em !important;
}

#dashboard #sidebar-contents ul li {
    float:left;
    width:50%;
}

#dashboard #sidebar-contents ul li input {
    height:40px;
    width:100%;
    font-family:Verdana,Geneva,sans-serif;
    font-size:0.8em;
    text-transform:uppercase;
    cursor:pointer;
    text-align:left;
    text-indent:10px;
}

#dashboard #imagery-info {
    clear:both;
    position:relative;
    top:-1.5rem;
    left:18%;
    width:40%;
    text-align:center;
    font-size:0.8em;
    color:white;
    padding:0.25rem;
    background-color:rgba(75,75,150,0.6);
}

#dashboard #page-stretcher {
    display:none;
}

/*********/
/* Admin */
/*********/

#admin {
    overflow:scroll;
    height:100%;
    width:100%;
    padding:0 20px 0 20px;
    background:rgba(255,255,255,0.9);
}

#admin h1,
#admin h2,
#admin h3,
#admin h4,
#admin h5,
#admin h6,
#admin h7,
#admin p,
#admin label {
    font-family:Open Sans !important;
    font-weight:400 !important;
}

#admin label {
    color:darkgray !important;
    font-size:1rem !important;
}

#admin h1 {
    font-size:2rem;
    font-weight:bold;
    margin:0px 0 10px 0;
}

#admin #project-selection label {
    font-size:1em;
    font-family:Oswald !important;
    font-weight:400 !important;
    color:#3fabc6 !important;
    text-transform:uppercase;
}

#admin #project-selection select {
    border:1px solid darkgray;
    height:2rem;
    font-size:0.9em;
}

#admin input#download-plot-data {
    position:absolute;
    top:0.9rem;
    padding:0.25rem;
    left:45rem;
    color:white;
    background-color:#55f;
    border-radius:0.5rem;
    box-shadow:0.25rem 0.25rem 0.5rem rgba(21,21,153,0.6);
}

#admin table {
    width:100% !important;
}

#admin legend {
    margin-top:1em;
    font-size:1em;
    font-family:Oswald !important;
    font-weight:400 !important;
    color:#3fabc6 !important;
    text-transform:uppercase;
}

#admin legend {
    font-size:1em;
    font-family:Oswald !important;
    font-weight:400 !important;
    color:#3fabc6 !important;
}

#admin #project-info label, #plot-info label, #sample-info th {
    font-size:0.8em !important;
    font-weight:400;
    margin-right:1rem;
    color:black !important;
}

#admin input[type=number], #admin input[type=text] {
    height:1rem;
    padding:0.3rem;
}

#admin fieldset {
    width:calc(50% - 4rem);
    margin-right:1rem;
}

#admin fieldset#project-info {
    float:left;
    display:inline;
    width:25%;
}

#admin fieldset#project-info td {
    display:inline;
}

#admin #project-description {
    width:100%;
}

#admin fieldset#plot-info {
    float:left;
    display:inline;
    width:25%;
}

#admin fieldset#plot-info td {
    display:inline;
}

#admin fieldset#bounding-box {
    clear:both;
    float:left;
    width:12.5vw;
    background-color:white;
    position:relative;
}

#admin fieldset#bounding-box input {
    width:100px;
}

/* bottom */
#admin fieldset#bounding-box input#lat-max {
    background-color:maroon;
}

/* top */
#admin fieldset#bounding-box input#lon-min {
    background-color:blue;
}

/* left */
#admin fieldset#bounding-box input#lon-max {
}

/* right */
#admin fieldset#bounding-box input#lat-min {
    background-color:olive;
}

#admin fieldset#bounding-box td {
}

#admin .empty {
    width:auto;
    height:auto;
}

#admin #value-name {
    font-size:0.9em !important;
}

#admin input#value-color {
    font-size:0.9em !important;
    width:20px;
    margin:0px;
}

#admin input#value-image {
    font-size:0.9em !important;
    width:40%;
}

#admin #new-project-map {
    float:left;
    border:1px solid;
    width:37.5%;
    z-index:49;
    margin-top:5px !important;
    height:auto;
}

#admin fieldset#project-info {
}

#admin fieldset#project-info tr {
}

#admin fieldset#project-info input[type=text] {
}

#admin #project-info #project-name {
}

#admin #project-info input[name="project-name"],
#project-info textarea[name="project-description"],
#plot-info input[name="plots"],
#plot-info input[name="buffer-radius"],
#plot-info input[name="samples-per-plot"] {
    width:95% !important;
}

/*project info */

#admin #project-info table td {
    display:block;
}
#admin #project-info textarea {
    height:4.3em;
    padding-right:8px;
}

#admin #plot-info table td {
    display:block;
}

#admin fieldset#project-info textarea {
}

/* box1 */
#admin fieldset#plot-info {
    float:left;
}

/* box2 */
#admin fieldset#plot-info table {
}

#admin fieldset#plot-info input {
    width:100%;
}

#admin fieldset#bounding-box {
}

#admin fieldset#bounding-box input {
}

#admin fieldset#bounding-box input#lat-max {
}

#admin fieldset#bounding-box input#lon-min {
}

#admin fieldset#bounding-box input#lon-max {
}

#admin fieldset#bounding-box input#lat-min {
}

/* box3 */
#admin fieldset#sample-info table tr th:nth-child(4) {
    text-align:center;
}

#admin #sample-info, #project-info, #plot-info {
    /* background-color:darkgray !important; */
}

#admin fieldset#sample-info td {
    text-align:center;
}

#admin fieldset#sample-info th.empty {
    background-color:#eee;
    border:0px;
}

#admin fieldset#sample-info input[type=file] {
    width:100%;
    text-align:right;
}

#admin fieldset#sample-info .circle {
    width:1rem;
    height:1rem;
    border-radius:0.5rem;
    margin:0rem auto;
}

#admin fieldset#sample-info input#value-name {
}

#admin fieldset#sample-info input#value-color {
    padding:0px !important;
}

#admin fieldset#sample-info input.button {
    padding:0.25rem;
    color:white;
    min-width:2rem;
    background-color:#55f;
    border-radius:0.5rem;
    box-shadow:0.25rem 0.25rem 0.5rem rgba(21,21,153,0.6);
}

#admin #sample-info th, td {
    display:table-cell;
}

#admin input.button[name=add-sample-value] {
    margin:0.5rem;
}

#admin #page-stretcher {
    clear:both;
}

#admin input.button[name=create-project] {
    font-size:1rem;
    padding:0.5rem;
    color:white;
    background-color:#55f;
    position:fixed;
    right:0;
    top:100px;
    /* border-radius:1rem; */
    /* box-shadow:0.25rem 0.25rem 0.5rem rgba(21,21,153,0.6); */
}

#admin #spinner {
    position:absolute;
    top:0.6rem;
    right:2rem;
    width:2rem;
    height:2rem;
    border-radius:1rem;
    border-bottom:0.25rem solid #55f;
    -webkit-animation:sweep 1s infinite linear;
    visibility:hidden;
}
@-webkit-keyframes sweep { to { -webkit-transform:rotate(360deg); } }

/* mmm */

label {
    display:block;
    font-family:Open Sans;
}

#create-project-form table {
    /* background-color:red; */
    width:100%;
}

/* mmm end */

@-webkit-keyframes sweep { to { -webkit-transform:rotate(360deg); } }

@media screen and (max-width:768px) {
    header {
        background-image:url(http://www.adpc.net/igo/images/SERVIR/usaid.png),
                         url(http://www.adpc.net/igo/images/SERVIR/nasa.png),
                         url(http://www.adpc.net/igo/images/SERVIR/ADPC.jpg),
                         url(http://www.adpc.net/igo/images/SERVIR/servir.png);
        background-position:20px 5px, 120px 5px, 150px 5px, right 10px;
        background-size:auto 30px, auto 30px, auto 30px, auto 20px;
    }

    nav {
        position:absolute;
        top:30px;
        left:5px;
        height:2.4rem;
        padding:0px;
    }

    #login-info {
        top:30px;
        right:0;
    }

    #login-info a:hover {
        border:none;
    }
}

@media screen and (max-width:600px) {
    #login-info p {
        margin-top:2.5rem;
    }

    header h1 {
        font-size:2rem;
    }

    #dashboard h1 {
        font-size:1.2rem;
    }

    #sidebar-contents fieldset:nth-child(2) {
        padding:0rem auto;
    }

    #dashboard #sidebar-contents fieldset:nth-child(2) ul li {
        border-left-style:solid !important;
        border-left-width:1vw !important;
    }

    #content #account-form {
        right:0;
    }
} /* end @width 960 */

/* Chrome, Safari, Opera */
@-webkit-keyframes menuin {
    from {width:0;}
    to {width:60vw;}
}

@keyframes menuin {
    from {width:0;}
    to {width:60vw;}
}

/* Chrome, Safari, Opera */
@-webkit-keyframes menuout {
    from {lwidth:60vw;}
    to {width:0;}
}

@keyframes menuout {
    from {width:60vw;}
    to {width:0;}
}

